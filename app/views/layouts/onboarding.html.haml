!!!
%html(lang="en")
  %head
    %title Configure Your Site or Campaign
    %link{:href => "/favicon.ico", :rel => "Shortcut Icon", :type => "image/x-icon"}/
    %meta{ charset: "utf-8" }/
    %meta{ content: "width=device-width, initial-scale=1", name: "viewport" }/

    /[if lt IE 9]
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>

    = stylesheet_link_tag "onboarding/application", media: :all

    = content_for :head_css
    = csrf_meta_tags
    - unless Rails.env.test?
      = kiss_metrics_tag

    %script{:src => "//use.typekit.net/rqm7drt.js"}
    :javascript
      try{Typekit.load();}catch(e){}

  %body{ class: onboarder_name }
    = render partial: 'onboarding/masthead'
    - if onboarding_group_steps.present?
      = render partial: 'onboarding/high_level_navigation'
    = render partial: 'onboarding/navigation'

    .container-fluid.container-content
      = yield
    = render partial: 'onboarding/footer'
    -# = yield :javascript
    = javascript_include_tag 'onboarding/application'
    = javascript_include_tag 'https://www.google.com/jsapi'
    = content_for :javascript
